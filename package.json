{
  "name": "electron-angular-material",
  "version": "0.2.0",
  "description": "Using Angular and Material with Electron by systemjs",
  "main": "main.js",
  "scripts": {
    "start": "set NODE_ENV=dev&& tsc && concurrently \"npm run tsc:w\" \"electron .\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "dist": "build -w --x64",
    "wb": "http-server wwwroot/ -p 8080",
    "prod": "tsc && concurrently \"npm run tsc:w\" \"electron .\" "
  },
  "keywords": [
    "Electron",
    "Angular",
    "Material",
    "systemjs",
    "windows",
    "autoupdate"
  ],
  "license": "MIT",
  "dependencies": {
    "@angular/core": "5.2.9",
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/animations": "5.2.9",
    "@angular/forms": "5.2.9",
    "@angular/http": "5.2.9",
    "@angular/cdk": "5.2.4",
    "@angular/material": "5.2.4",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "@angular/platform-server": "5.2.9",
    "@angular/router": "5.2.9",
    "core-js": "2.5.1",
    "hammerjs": "2.0.8",
    "rxjs": "5.5.2",
    "systemjs": "0.20.19",
    "typescript": "2.6.1",
    "zone.js": "0.8.18",
    "electron-log": "2.2.14",
    "electron-updater": "2.21.4",
    "http-server": "0.11.1"
  },
  "devDependencies": {
    "@types/core-js": "0.9.43",
    "@types/node": "8.0.49",
    "concurrently": "3.5.0",
    "electron": "1.8.4",
    "electron-builder": "20.8.0"
  },
  "build": {
    "appId": "EAA",
    "productName": "Electron Angular Application",
    "win": {
      "target": [
        "nsis"
      ]
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1:8080/"
      }
    ],
    "nsis": {
      "oneClick": false,
      "artifactName": "Electron Angular Application Setup.exe"
    }
  }
}
